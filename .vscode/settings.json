{
    "arduino.commandPath": "arduino-cli.exe",
    "arduino.logLevel": "info",
    "arduino.path": "D:\\Arduino",
    "arduino.useArduinoCli": true,
    "files.associations": {
        "*.ino": "cpp",
        "iostream": "cpp",
        "memory": "cpp",
        "format": "cpp",
        "functional": "cpp",
        "charconv": "c",
        "optional": "c",
        "system_error": "c",
        "array": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c"
    },
    "arduino.disableIntelliSenseAutoGen": true,
    "arduino.clearOutputOnBuild": true,
}